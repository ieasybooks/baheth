<div id="<%= dom_id playlist %>">
  <p>
    <strong>Title:</strong>
    <%= playlist.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= playlist.description %>
  </p>

  <p>
    <strong>Link:</strong>
    <%= playlist.link %>
  </p>

  <p>
    <strong>Hidden:</strong>
    <%= playlist.hidden %>
  </p>

  <p>
    <strong>Media:</strong>
    <%= playlist.medium_ids %>
  </p>

  <p>
    <strong>Speakers:</strong>
    <%= playlist.speaker_ids %>
  </p>

  <p>
    <strong>User:</strong>
    <%= playlist.user_id %>
  </p>

</div>
